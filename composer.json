{
    "name": "logipro/ml-sandbox",
    "type": "app",
    "description": "A sandbox for Machine Learning",
    "keywords": ["php", "DDT", "Rubix ML", "PHP-ML", "Machine Learning"],
    "homepage": "https://github.com/frederic100/ml-sandbox",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Frédéric Royet",
            "email": "frederic.royet@logipro.com",
            "homepage": "https://www.linkedin.com/in/frederic-royet-42568566/",
            "role": "Project Founder"
        }
    ],
    "require": {
        "php": ">=8.3",
        "rubix/ml": "^2.5",
        "php-ai/php-ml": "^0.10.0"
    },
    "require-dev": {
        "infection/extension-installer": "0.1.2",
        "infection/infection": "^0.28",
        "phpstan/phpdoc-parser": "^1.28",
        "phpstan/phpstan": "1.10",
        "phpunit/phpunit": "^11",
        "squizlabs/php_codesniffer": "3.*",
        "behat/behat": "^3.14"
    },
    "autoload": {
        "psr-4" : {
            "MLSandbox\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "Tests\\MLSandbox\\" : "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
